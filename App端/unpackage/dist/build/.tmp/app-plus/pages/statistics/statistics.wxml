<view><block wx:if="{{isHaveData==false}}"><cu-custom vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view class="text-xl" slot="content">数据统计</view></cu-custom><no-thing vue-id="2" swiperHeight="{{swiperHeight}}" bind:__l="__l"></no-thing></block><block wx:else><view class="page-one"><text data-event-opts="{{[['tap',[['backIndex',['$event']]]]]}}" class="cuIcon-close" bindtap="__e"></text></view><view class="page-two"><view class="page-title">数据统计</view><view class="u-f u-f-jsb"><view class="class-info"><text>{{roomName}}</text><text class="cuIcon-right"></text></view><view class="u-f-ajc"><view data-event-opts="{{[['tap',[['switcher',['$event']]]]]}}" class="oval" bindtap="__e"><text>切换</text><text class="cuIcon-unfold"></text></view></view></view></view><view class="page-three qiun-charts"><canvas class="charts" canvas-id="canvasPie" id="canvasPie" data-event-opts="{{[['touchstart',[['touchPie',['$event']]]]]}}" bindtouchstart="__e"></canvas></view><view class="page-four u-f-ajc"><view class="card u-f u-f-jsb"><view class="card-item u-f-ajc u-f-column"><view style="color:#9152FA;">{{headCount+"人"}}</view><view>总人数</view></view><view class="card-item u-f-ajc u-f-column"><view style="color:#2692FF;">{{signCount+"人"}}</view><view>已签到</view></view><view class="card-item u-f-ajc u-f-column"><view style="color:#fc7c1c;">{{headCount-signCount+"人"}}</view><view>未签到</view></view><view class="card-item u-f-ajc u-f-column"><view style="color:#0cd452;">{{signCount/headCount*100+"%"}}</view><view>签到率</view></view></view></view></block></view>