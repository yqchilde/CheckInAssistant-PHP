<view><cu-custom vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view class="text-xl" slot="content">{{roomName}}</view></cu-custom><view class="u-f-column u-f-ac"><view class="card-head u-f u-f-jsb"><view class="card-title">创建时间</view><view class="card-info">{{joinTime}}</view></view><view class="card-head"><view class="card-title">组成班级</view><block wx:for="{{classes}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="card-info">{{''+item+''}}</view></block></view><view class="card-list"><view class="cu-list menu-avatar"><block wx:for="{{peoList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item card-list-item margin-bottom-sm"><view class="cu-avatar round lg" style="{{'background-image:'+('url(/static/images/me/header-default.png)')+';'}}"></view><view class="content"><view class="text-black text-lg">{{item.name}}</view><view class="text-gray text-sm"><text>{{item.joinTime+"加入"}}</text></view></view><view class="action"><view class="cuIcon-roundcheckfill text-green" style="font-size:37rpx;"></view></view><view class="move"><view data-event-opts="{{[['tap',[['delUser',['$0'],[[['peoList','',index,'userId']]]]]]]}}" class="bg-red" bindtap="__e">删除</view></view></view></block></view></view></view></view>